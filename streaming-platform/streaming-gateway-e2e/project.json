{
  "name": "streaming-gateway-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["streaming-gateway"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "streaming-gateway-e2e/jest.config.cts",
        "passWithNoTests": true
      },
      "dependsOn": ["streaming-gateway:build", "streaming-gateway:serve"]
    }
  }
}
